#!/usr/bin/env ruby

require "bundler/setup"
require "dotenv/load"
require "beyond_api"

unless ENV["CLIENT_ID"].nil? and ENV["CLIENT_SECRET"].nil?
  BeyondAPI.setup do |config|
    config.client_id = ENV["CLIENT_ID"]
    config.client_secret = ENV["CLIENT_SECRET"]
  end
end

require "pry"
Pry.start

#!/usr/bin/env ruby
# frozen_string_literal: true

require "bundler/setup"
require "dotenv/load"
require "beyond_api"
require "pry"

Dotenv.load('.env.development')

BeyondApi.setup do |config|
  config.client_id = ENV.fetch("CLIENT_ID", nil)
  config.client_secret = ENV.fetch("CLIENT_SECRET", nil)
end

Pry.start
